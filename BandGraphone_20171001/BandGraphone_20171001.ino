/**/#include "ada88.h"#define SERIAL_SPEED (38400)    // for Serial MIDI//#define SERIAL_SPEED (31250)    // for MIDI Shield#define NUM_MODULE            (5)#define NUM_SENSOR_PER_MODULE (8)#define PIN_CH_SELECT_0       (10)#define ANALOG_PIN_MODULE_0   (1)void setup() {  pinMode(PIN_CH_SELECT_0 + 0, OUTPUT);  pinMode(PIN_CH_SELECT_0 + 1, OUTPUT);  pinMode(PIN_CH_SELECT_0 + 2, OUTPUT);  Serial.begin(SERIAL_SPEED);  ada88_init();  ada88_write(19);}void loop() {  digitalWrite(PIN_CH_SELECT_0 + 0, LOW);  digitalWrite(PIN_CH_SELECT_0 + 1, LOW);  digitalWrite(PIN_CH_SELECT_0 + 2, LOW);  delay(2);  int brightness = analogRead(0);  ada88_writeNumber(10 - brightness / 100);  digitalWrite(PIN_CH_SELECT_0 + 0, HIGH);  digitalWrite(PIN_CH_SELECT_0 + 1, HIGH);  digitalWrite(PIN_CH_SELECT_0 + 2, HIGH);  Serial.write(42);  Serial.flush();  delay(100);}